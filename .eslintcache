rd')\r\n    }\r\n    render() {\r\n        const { loggedInUser } = this.props\r\n\r\n        let signupSection = (\r\n            <form className=\"frm\" onSubmit={this.doSignup}>\r\n                <h2>Signup</h2>\r\n                <input className=\"input-log\" autoFocus\r\n                    type=\"text\"\r\n                    name=\"fullname\"\r\n                    value={this.state.signupCred.fullname}\r\n                    onChange={this.signupHandleChange}\r\n                    placeholder=\"Full name\"\r\n                    autoComplete=\"fullname\"\r\n                />\r\n                <input className=\"input-log\"\r\n                    name=\"password\"\r\n                    type=\"password\"\r\n                    value={this.state.signupCred.password}\r\n                    onChange={this.signupHandleChange}\r\n                    placeholder=\"Password\"\r\n                    autoComplete=\"current-password\"\r\n                />\r\n                <input className=\"input-log\"\r\n                    type=\"text\"\r\n                    name=\"username\"\r\n                    value={this.state.signupCred.username}\r\n                    onChange={this.signupHandleChange}\r\n                    placeholder=\"Username\"\r\n                    autoComplete=\"username\"\r\n                />\r\n                <br />\r\n                <button>Signup</button>\r\n            </form>\r\n        )\r\n        return (\r\n            <div className=\"signup\">\r\n                {!loggedInUser && signupSection}\r\n            </div>\r\n        )\r\n    }\r\n}\r\nconst mapStateToProps = state => {\r\n    return {\r\n        loggedInUser: state.userModule.loggedInUser\r\n    }\r\n}\r\nconst mapDispatchToProps = {\r\n    signup\r\n}\r\nexport const Signup = connect(mapStateToProps, mapDispatchToProps)(_Signup)","D:\\עדינה\\coding academy\\sprint-4-Trello\\src\\pages\\Board.jsx",[],"D:\\עדינה\\coding academy\\sprint-4-Trello\\src\\store\\reducers\\